{
	"name": "R Data Science Environment",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "4" }
	},

	"forwardPorts": [8787],
	"portsAttributes": {
		"8787": {
			"label": "Rstudio",
			"requireLocalPort": true,
			"onAutoForward": "ignore"
		}
	},

	"postAttachCommand": "sudo rstudio-server start"

}
